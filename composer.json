{
    "name": "spargon/laravel-auth-logger",
    "description": "Laravel Auth Logger stores user authentication logs and sends out notifications whenever a user logs in from a new system.",
    "keywords": [
        "spargon",
        "laravel-auth-logger"
    ],
    "homepage": "https://github.com/spargon/laravel-auth-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "TechTailor",
            "email": "hello@moinuddin.info",
            "homepage": "https://spargon.tech",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "jenssegers/agent": "^2.6",
        "illuminate/auth": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/bus": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/console": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/contracts": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/database": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/http": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/notifications": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "spatie/laravel-package-tools": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Spargon\\AuthLogger\\": "src",
            "Spargon\\AuthLogger\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spargon\\AuthLogger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spargon\\AuthLogger\\AuthLoggerServiceProvider"
            ]
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to use the Slack transport (~6.0)",
        "laravel/slack-notification-channel": "Required to send notifications through Slack"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
